influx db

import "date"

from(bucket: "EAP")
  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)
  |> aggregateWindow(every: 1m, fn: last, createEmpty: false)
  |> map(fn: (r) => ({
        _time: r._time,
        _value: (
          float(v: date.hour(t: r._time)) * 60.0 +
          float(v: date.minute(t: r._time))
        ) * 0.000694774,
        
        _field: "time2",
        _measurement: r._measurement
    }))

